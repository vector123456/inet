%file: test.ned

import inet.common.newqueue.OrdinalBasedDropper;
import inet.common.newqueue.PacketConsumer;
import inet.common.newqueue.PacketProducer;

network TestOrdinalBasedDropper
{
    submodules:
        producer: PacketProducer {
            @display("p=100,100");
        }
        dropper: OrdinalBasedDropper {
            @display("p=200,100");
        }
        consumer: PacketConsumer {
            @display("p=300,100");
        }
    connections:
        producer.out --> dropper.in;
        dropper.out --> consumer.in;
}

%inifile: omnetpp.ini

[General]
network = TestOrdinalBasedDropper
sim-time-limit = 10s
cmdenv-event-banners = false
*.producer.packetLength = 1B
*.producer.productionInterval = 1s
*.dropper.dropsVector = "0; 2; 4; 6; 8; 10"

