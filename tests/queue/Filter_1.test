%file: test.ned

import inet.common.newqueue.PacketConsumer;
import inet.common.newqueue.PacketFilter;
import inet.common.newqueue.PacketProducer;

network TestFilter
{
    submodules:
        producer: PacketProducer {
            @display("p=100,100");
        }
        filter: PacketFilter {
            @display("p=200,100");
        }
        consumer: PacketConsumer {
            @display("p=300,100");
        }
    connections allowunconnected:
        producer.out --> filter.in;
        filter.out --> consumer.in;
}

%file: Test.cc
#include "inet/common/newqueue/PacketFilterFunction.h"
#include "inet/common/packet/Packet.h"

using namespace inet; 

static bool testFilter(Packet *packet)
{
    return packet->getId() % 2 == 0;
}

Register_Packet_Filter_Function(TestFilter, testFilter);

%inifile: omnetpp.ini

[General]
network = TestFilter
sim-time-limit = 10s
cmdenv-event-banners = false
*.producer.packetLength = 1B
*.producer.productionInterval = 1s
*.filter.filterClass = "TestFilter"

