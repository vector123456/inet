%file: test.ned

import inet.common.newqueue.PacketConsumer;
import inet.common.newqueue.PacketProvider;
import inet.common.newqueue.PacketServer;

network TestServer
{
    submodules:
        provider: PacketProvider {
            @display("p=100,100");
        }
        server: PacketServer {
            @display("p=200,100");
        }
        consumer: PacketConsumer {
            @display("p=300,100");
        }
    connections allowunconnected:
        provider.out --> server.in;
        server.out --> consumer.in;
}

%inifile: omnetpp.ini

[General]
network = TestServer
sim-time-limit = 10s
cmdenv-event-banners = false
*.provider.packetLength = 1B
*.server.processingTime = uniform(0s, 2s)

