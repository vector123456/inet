%file: test.ned

import inet.common.newqueue.OrdinalBasedDuplicator;
import inet.common.newqueue.PacketConsumer;
import inet.common.newqueue.PacketProducer;

network TestOrdinalBasedDuplicator
{
    submodules:
        producer: PacketProducer {
            @display("p=100,100");
        }
        duplicator: OrdinalBasedDuplicator {
            @display("p=200,100");
        }
        consumer: PacketConsumer {
            @display("p=300,100");
        }
    connections:
        producer.out --> duplicator.in;
        duplicator.out --> consumer.in;
}

%inifile: omnetpp.ini

[General]
network = TestOrdinalBasedDuplicator
sim-time-limit = 10s
cmdenv-event-banners = false
*.producer.packetLength = 1B
*.producer.productionInterval = 1s
*.duplicator.duplicatesVector = "0; 2; 4; 6; 8; 10"

