%file: test.ned

import inet.common.newqueue.PacketConsumer;
import inet.common.newqueue.PacketDuplicator;
import inet.common.newqueue.PacketProducer;

network TestDuplicator
{
    submodules:
        producer: PacketProducer {
            @display("p=100,100");
        }
        duplicator: PacketDuplicator {
            @display("p=200,100");
        }
        consumer: PacketConsumer {
            @display("p=300,100");
        }
    connections allowunconnected:
        producer.out --> duplicator.in;
        duplicator.out --> consumer.in;
}

%inifile: omnetpp.ini

[General]
network = TestDuplicator
sim-time-limit = 10s
cmdenv-event-banners = false
*.producer.packetLength = 1B
*.producer.productionInterval = 1s
*.duplicator.numDuplicates = intuniform(0, 1)

